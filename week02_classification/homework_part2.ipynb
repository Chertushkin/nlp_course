    "\n",
    "class SalaryDataset(Dataset):\n",
    "    def __init__(self, data, categorical_data):\n",
    "        super().__init__()\n",
    "        self.data = data\n",
    "        self.categorical_data = categorical_data\n",
    "\n",
    "    def __getitem__(self, index):\n",
    "        title = torch.as_tensor(as_matrix(data[\"Title\"].values[index]))\n",
    "        full_descr = torch.as_tensor(as_matrix(data[\"FullDescription\"].values[index]))\n",
    "        cats = torch.as_tensor(self.categorical_data[index])\n",
    "        target = torch.as_tensor(data[TARGET_COLUMN].values[index])\n",
    "        return {'Title': title, 'FullDescription': full_descr, 'Categorical': cats, TARGET_COLUMN: target}\n",
    "\n",
    "    def __len__(self):\n",
    "        return len(self.data)"
