    "def train_model_vanilla(model, loss, optimizer, num_epochs):\n",
    "    epoch_train_losses = []\n",
    "    epoch_val_losses = []\n",
    "\n",
    "    step_train_losses = []\n",
    "    step_val_losses = []\n",
    "    for epoch in range(num_epochs):\n",
    "        for phase in ['train', 'val']:\n",
    "            if phase == 'train':\n",
    "                model.train()\n",
    "                the_ds = train_ds\n",
    "            else:\n",
    "                model.eval()\n",
    "                the_ds = val_ds\n",
    "\n",
    "            running_loss = 0.\n",
    "            sampler = BatchSampler(\n",
    "                RandomSampler(the_ds),\n",
    "                batch_size=BATCH_SIZE,\n",
    "                drop_last=False)\n",
    "\n",
    "            dataloader = DataLoader(the_ds, sampler=sampler, num_workers=8)\n",
    "            num_batches = len(the_ds) // BATCH_SIZE\n",
    "            for i, batch in enumerate(dataloader):\n",
    "                inputs = batch\n",
    "                labels = batch[TARGET_COLUMN]\n",
    "\n",
    "                labels = labels.squeeze().to(DEVICE)\n",
    "                inputs = {k: v.squeeze().to(DEVICE) for k, v in inputs.items()}\n",
    "\n",
    "                with torch.set_grad_enabled(phase == 'train'):\n",
    "                    if phase == 'train':\n",
    "                        optimizer.zero_grad()                   \n",
    "                        preds = model(inputs)\n",
    "                        loss_value = loss(preds, labels)\n",
    "                        loss_value.backward()\n",
    "                        optimizer.step()\n",
    "                        step_train_losses.append(loss_value.item())\n",
    "                    else:\n",
    "                        preds = model(inputs)\n",
    "                        loss_value = loss(preds, labels)\n",
    "                        step_val_losses.append(loss_value.item())\n",
    "\n",
    "                running_loss += loss_value.item()\n",
    "            \n",
    "            epoch_loss = running_loss / num_batches\n",
    "            print(f'Epoch {epoch}/{num_epochs} {phase} Loss: {epoch_loss}')\n",
    "            if phase == 'train':\n",
    "                epoch_train_losses.append(epoch_loss)\n",
    "            else:\n",
    "                epoch_val_losses.append(epoch_loss)\n",
    "\n",
    "    return epoch_train_losses, epoch_val_losses, step_train_losses, step_val_losses"
   ]
